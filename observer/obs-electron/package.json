{
  "name": "obs-electron",
  "version": "0.0.27",
  "channel": "alpha",
  "main": "src-electron/electron-main.js",
  "license": "MIT",
  "description": "Observer",
  "productName": "Observer NG",
  "author": "FRAM Data Team",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-webapp": "ng build --prod",
    "electron-build-watch": "ng build --base-href . --watch",
    "electron-run": "electron .",
    "electron-build-run": "ng build --base-href . && electron .",
    "electron-build-prod": "ng build --prod --base-href . && electron .",
    "electron-package-only": "ng build --prod --base-href . && npx electron-builder --windows nsis --dir",
    "create-installer-win": "ng build --prod --base-href . && npx electron-builder --windows nsis",
    "postinstall": "node build/patch/patch-electron-fs.js"
  },
  "build": {
    "appId": "ObserverElectron",
    "directories": {
      "output": "builder_output/"
    },
    "files": [
      "build",
      "src-electron/electron-main.js",
      "node_modules/material-design-icons/iconfont/**",
      "!node_modules/material-design-icons/**"
    ],
    "publish": [
      {
        "provider": "generic",
        "url": "https://nwcdevfram.nwfsc.noaa.gov:10443/"
      }
    ],
    "nsis": {
      "perMachine": false,
      "allowElevation": false
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.3",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "^7.1.3",
    "@angular/compiler": "^7.1.3",
    "@angular/core": "^7.1.3",
    "@angular/flex-layout": "^7.0.0-beta.21",
    "@angular/forms": "^7.1.3",
    "@angular/http": "^7.1.3",
    "@angular/material": "^7.1.1",
    "@angular/material-moment-adapter": "^7.1.1",
    "@angular/platform-browser": "^7.1.3",
    "@angular/platform-browser-dynamic": "^7.1.3",
    "@angular/router": "^7.1.3",
    "@ng-select/ng-select": "^2.13.2",
    "@types/file-saver": "^2.0.0",
    "@types/jquery": "^3.3.28",
    "@types/moment": "^2.13.0",
    "core-js": "^2.6.1",
    "cryptiles": ">=4.1.2",
    "crypto-pouch": "^3.1.3",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.6",
    "file-saver": "^2.0.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.23.0",
    "nan": "^2.12.1",
    "ngx-electron": "^2.0.0",
    "pdfmake": "^0.1.39",
    "pouchdb": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "primeicons": "^1.0.0",
    "primeng": "^7.0.3",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "virtual-keyboard": "^1.28.7",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.3",
    "@angular/cli": "^7.1.3",
    "@angular/compiler-cli": "^7.1.3",
    "@angular/language-service": "^7.1.3",
    "@types/jasmine": "^3.3.4",
    "@types/node": "~8.9.4",
    "@types/pdfmake": "^0.1.2",
    "@types/virtual-keyboard": "^1.26.0",
    "codelyzer": "~4.5.0",
    "electron": "^3.0.13",
    "electron-builder": "^20.38.3",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
