{
  "name": "obs-electron",
  "version": "0.0.25",
  "channel": "alpha",
  "main": "src-electron/electron-main.js",
  "license": "MIT",
  "description": "Observer",
  "productName": "Observer NG",
  "author": "FRAM Data Team",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-webapp": "ng build --prod",
    "electron-build-watch": "ng build --base-href . --watch",
    "electron-build-run": "ng build --base-href . && electron .",
    "electron-build-prod": "ng build --prod --base-href . && electron .",
    "electron-run": "electron .",
    "electron-package-only": "ng build --prod --base-href . && npx electron-builder --windows nsis --dir",
    "create-installer-win": "ng build --prod --base-href . && npx electron-builder --windows nsis",
    "postinstall": "node build/patch/patch-electron-fs.js"
  },
  "build": {
    "appId": "ObserverElectron",
    "directories": {
      "output": "builder_output/"
    },
    "files": [
      "src-electron/electron-main.js",
      "build",
      "node_modules/material-design-icons/iconfont/**",
      "!node_modules/material-design-icons/**"
    ],
    "publish": [
      {
        "provider": "generic",
        "url": "https://nwcdevfram.nwfsc.noaa.gov:10443/"
      }
    ],
    "nsis": {
      "perMachine": false,
      "allowElevation": false
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/material": "^6.4.7",
    "@angular/material-moment-adapter": "^6.4.7",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@ng-select/ng-select": "^2.12.0",
    "@swimlane/ngx-datatable": "^12.0.0",
    "@types/file-saver": "^1.3.1",
    "@types/moment": "^2.13.0",
    "core-js": "^2.5.7",
    "crypto-pouch": "^3.1.3",
    "electron-log": "^2.2.17",
    "electron-updater": "^2.23.3",
    "file-saver": "^1.3.8",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-dist": "^1.12.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "nan": "^2.11.1",
    "ngx-electron": "^1.0.4",
    "pdfmake": "^0.1.38",
    "pouchdb": "^6.4.3",
    "pouchdb-find": "^6.4.3",
    "primeicons": "^1.0.0",
    "primeng": "^6.1.6",
    "roboto-fontface": "^0.9.0",
    "rxjs": "^6.3.3",
    "virtual-keyboard": "^1.28.7",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.5",
    "@angular/cli": "^6.2.7",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@types/jasmine": "^2.8.11",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~8.9.4",
    "@types/pdfmake": "^0.1.2",
    "@types/virtual-keyboard": "^1.26.0",
    "codelyzer": "~4.2.1",
    "electron": "^3.0.8",
    "electron-builder": "^20.33.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2"
  }
}
